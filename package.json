{
  "name": "cnci-app",
  "version": "1.0.0",
  "main": "src/main.tsx",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx}\"",
    "lint": "echo \"\\033[33mRunning eslint check\" && npx eslint .",
    "lint:fix": "echo \"\\033[33mRunning eslint fix\" && npx eslint --fix --ext .tsx,.ts,.js,.jsx ./src",
    "test:tsc": "echo \"\\033[33mRunning typescript check\" && tsc",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@expo/html-elements": "^0.5.1",
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^19.0.0",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native-stack": "^6.9.13",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "@types/react": "~18.2.14",
    "@types/react-helmet": "^6.1.7",
    "axios": "^1.5.0",
    "expo": "~49.0.8",
    "expo-linking": "~5.0.2",
    "expo-status-bar": "~1.6.0",
    "metro-core": "^0.79.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-native": "0.72.4",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
